{
    "expo": {
        "name": "환경발자국",
        "slug": "client",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/images/icon_2.png",
        "scheme": "myapp",
        "userInterfaceStyle": "automatic",
        "jsEngine": "hermes",
        "splash": {
            "image": "./assets/images/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "ios": {
            "supportsTablet": true,
            "bundleIdentifier": "com.jobgrass.client"
        },
        "android": {
            "package": "com.jobgrass.client",
            "config": {
                "googleMaps": {
                    "apiKey": "AIzaSyAZFt0qhnszhyMTFMfYqrj7sS0-wKAUXUo"
                }
            }
        },
        "web": {
            "bundler": "metro",
            "output": "static",
            "favicon": "./assets/images/favicon.png"
        },
        "experiments": {
            "typedRoutes": true
        },
        "extra": {
            "eas": {
                "projectId": "cfda5041-af8f-4cb3-91f0-6fc7ee643520"
            }
        },
        "plugins": [
            "expo-build-properties",
            "react-native-google-mobile-ads",
            [
                "expo-location",
                {
                    "isAndroidBackgroundLocationEnabled": true
                }
            ],
            "expo-router"
        ],
        "androidNavigationBar": {
            "visible": "sticky-immersive"
        }
    }
}